[ignore]
.*public/static.*
.*/node_modules/.*

[include]

[libs]

[options]
suppress_comment= \\(.\\|\n\\)*\\flow:disable .+
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
module.name_mapper='^.*\.scss$' -> '<PROJECT_ROOT>/src/ignoreExtensions'
module.name_mapper='^.*\.css$' -> '<PROJECT_ROOT>/src/ignoreExtensions'
